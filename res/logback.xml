<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <appender name="sout" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{56} - %msg%n</pattern>
        </encoder>
    </appender>

    <root level="ALL">
        <appender-ref ref="sout"/>
    </root>
</configuration>